<project 
xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.beangle</groupId>
    <artifactId>beangle-scala</artifactId>
    <version>24.2</version>
  </parent>
  <groupId>org.beangle.cache</groupId>
  <artifactId>beangle-cache-parent_2.12</artifactId>
  <packaging>pom</packaging>
  <version>0.0.10</version>
  <name>Beangle Cache Parent</name>
  <modules>
    <module>api</module>
    <module>ehcache</module>
    <module>caffeine</module>
    <module>redis</module>
    <module>jgroups</module>
  </modules>
  <description> The Beangle Cache Library. </description>
  <url>https://github.com/beangle/cache</url>
