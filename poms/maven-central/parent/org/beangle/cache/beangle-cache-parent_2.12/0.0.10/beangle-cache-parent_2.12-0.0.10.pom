    <url>https://github.com/beangle/cache/issues</url>
  </issueManagement>
  <scm>
    <connection>scm:git:git://github.com/beangle/cache.git</connection>
    <developerConnection>
      scm:git:git@github.com:beangle/cache.git
    </developerConnection>
    <url>https://github.com/beangle/cache</url>
    <tag>HEAD</tag>
  </scm>
  <properties>
