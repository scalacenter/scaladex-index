xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.beangle</groupId>
    <artifactId>beangle-scala</artifactId>
    <version>24.2</version>
  </parent>
  <groupId>org.openurp</groupId>
  <artifactId>openurp-parent</artifactId>
  <packaging>pom</packaging>
  <version>0.2.4</version>
  <name>OpenURP Parent</name>
  <prerequisites>
    <maven>3.3</maven>
  </prerequisites>
  <properties>
    <bundle.symbolicName>org.openurp.parent</bundle.symbolicName>
    <bundle.namespace>org.openurp</bundle.namespace>
    <scala.version>2.12.3</scala.version>
    <beangle.cdi.version>0.0.13</beangle.cdi.version>
    <beangle.commons.version>5.0.0.M6</beangle.commons.version>
    <beangle.data.version>5.0.0.M6</beangle.data.version>
    <beangle.cache.version>0.0.10</beangle.cache.version>
    <beangle.webmvc.version>0.3.14</beangle.webmvc.version>
    <beangle.webui.version>0.0.14</beangle.webui.version>
    <beangle.security.version>4.1.2</beangle.security.version>
    <beangle.maven.version>0.3.5</beangle.maven.version>
    <beangle.template.version>0.0.18</beangle.template.version>
    <beangle.ids.version>0.0.12</beangle.ids.version>
    <beangle.serializer.version>0.0.3</beangle.serializer.version>
    <HikariCP.version>2.6.2</HikariCP.version>
    <hibernate.version>5.2.10.Final</hibernate.version>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.beangle.commons</groupId>
        <artifactId>beangle-commons-core_2.12</artifactId>
        <version>${beangle.commons.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.commons</groupId>
