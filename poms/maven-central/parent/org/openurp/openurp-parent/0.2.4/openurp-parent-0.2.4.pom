      <dependency>
        <groupId>org.beangle.security</groupId>
        <artifactId>beangle-security-core_2.12</artifactId>
        <version>${beangle.security.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.security</groupId>
        <artifactId>beangle-security-web_2.12</artifactId>
        <version>${beangle.security.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.security</groupId>
        <artifactId>beangle-security-cas_2.12</artifactId>
        <version>${beangle.security.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.security</groupId>
        <artifactId>beangle-security-session_2.12</artifactId>
        <version>${beangle.security.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.security</groupId>
        <artifactId>beangle-security-protobuf_2.12</artifactId>
        <version>${beangle.security.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.ids.cas</groupId>
        <artifactId>beangle-ids-cas-core_2.12</artifactId>
        <version>${beangle.ids.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.ids.cas</groupId>
        <artifactId>beangle-ids-cas-web_2.12</artifactId>
