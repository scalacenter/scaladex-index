<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>org.beangle</groupId>
    <artifactId>beangle-scala</artifactId>
    <version>22.3</version>
  </parent>
  <groupId>org.beangle.spring</groupId>
  <artifactId>beangle-spring-parent_2.11</artifactId>
  <packaging>pom</packaging>
  <version>4.0.11</version>
  <name>Beangle Spring Parent</name>

  <modules>
    <module>context</module>
    <module>hibernate</module>
  </modules>

  <description>
    The Beangle Spring Integration.
  </description>

  <url>https://github.com/beangle/spring</url>

  <issueManagement>
    <system>github</system>
    <url>https://github.com/beangle/spring/issues</url>
  </issueManagement>

  <scm>
    <connection>scm:git:git://github.com/beangle/spring.git</connection>
    <developerConnection>scm:git:git@github.com:beangle/spring.git</developerConnection>
    <url>https://github.com/beangle/spring</url>
    <tag>HEAD</tag>
  </scm>

  <properties>
    <spring.version>4.0.4.RELEASE</spring.version>
    <commons.version>4.3.0</commons.version>
    <data.version>4.2.3</data.version>
  </properties>

  <dependencyManagement>
    <dependencies>
      <!--beangle-->
      <dependency>
        <groupId>org.beangle.commons</groupId>
        <artifactId>beangle-commons-core_2.11</artifactId>
        <version>${commons.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.commons</groupId>
        <artifactId>beangle-commons-web_2.11</artifactId>
        <version>${commons.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.data</groupId>
        <artifactId>beangle-data-model_2.11</artifactId>
        <version>${data.version}</version>
      </dependency>
      <dependency>
        <groupId>org.beangle.data</groupId>
        <artifactId>beangle-data-jpa_2.11</artifactId>
        <version>${data.version}</version>
      </dependency>
      <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-core</artifactId>
        <version>${spring.version}</version>
      </dependency>
      <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-context</artifactId>
        <version>${spring.version}</version>
      </dependency>
      <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-tx</artifactId>
        <version>${spring.version}</version>
      </dependency>
      <dependency>
        <groupId>org.springframework</groupId>
        <artifactId>spring-jdbc</artifactId>
        <version>${spring.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <dependencies>
    <dependency>
      <groupId>org.springframework</groupId>
      <artifactId>spring-core</artifactId>
    </dependency>
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-api</artifactId>
    </dependency>
    <dependency>
      <groupId>ch.qos.logback</groupId>
      <artifactId>logback-core</artifactId>
    </dependency>
    <dependency>
      <groupId>ch.qos.logback</groupId>
      <artifactId>logback-classic</artifactId>
    </dependency>
  </dependencies>
</project>
