<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.apache.zeppelin</groupId>
  <artifactId>zeppelin-display_2.11</artifactId>
  <version>0.7.2</version>
  <name>Zeppelin: Display system apis</name>
  <description>Zeppelin project</description>
  <url>http://zeppelin.apache.org/zeppelin-display_2.11</url>
  <licenses>
    <license>
      <name>The Apache Software License, Version 2.0</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
      <distribution>repo</distribution>
    </license>
  </licenses>
  <scm>
    <connection>scm:git:https://git-wip-us.apache.org/repos/asf/zeppelin.git/zeppelin-display_2.11</connection>
    <developerConnection>scm:git:https://git-wip-us.apache.org/repos/asf/zeppelin.git/zeppelin-display_2.11</developerConnection>
    <url>https://git-wip-us.apache.org/repos/asf/zeppelin.git/zeppelin-display_2.11</url>
  </scm>
  <dependencies>
    <dependency>
      <groupId>org.apache.zeppelin</groupId>
      <artifactId>zeppelin-interpreter</artifactId>
